require ast_factory

constructor PrintNode(exp, endl, line)
    statement? = true
    expression? = false
    :show
        "<Print " + exp + " \\n>"
    end

    :typecheck(symbol_table, struct_map)
        exp.typecheck(symbol_table, struct_map)
    end

end

:parse_print_node(json)
    PrintNode!(create_node!(json["exp"]), json["endl"], json["line"])
end