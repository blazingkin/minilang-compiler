require ast_factory
require ../types/primitive_type

constructor NumberNode(number, line)
    statement? = false
    expression? = true
    :show
        "<Number " + number + ">"
    end

    :typecheck(id_map, struct_map)
        primitive_type("int")
    end

end

:parse_number_node(json)
    NumberNode!(json["value"], json["line"])
end