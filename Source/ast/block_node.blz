require ast_factory
require ../types/primitive_type

constructor BlockNode(subnodes)

    :show
        "<Block " + subnodes.map(x -> x + "\n") + ">"
    end

    :typecheck(id_map, struct_map)
        primitive_type("int")
    end

end

:parse_block_node(subnodes)
    BlockNode(subnodes.map!(create_node))
end