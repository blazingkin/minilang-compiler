require struct_table_info

struct_names = []
struct_info = []

constructor StructTable()

    :getInfo (name)
        for i = 0; i < struct_names.length; i++;
            if struct_names[i] == name
                return struct_info [i]
            end
        end
        return null;
    end

    :addInfo (name, info)
        struct_names [struct_names.length()] = name
        struct_info [struct_info.length()] = info
    end
        

end